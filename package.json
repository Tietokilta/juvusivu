{
  "name": "juvusivu",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build --experimental-build-mode compile",
    "codegen": "pnpm generate:importmap && pnpm generate:types",
    "db:seed": "payload run src/lib/api/standaloneSeed.ts",
    "dev": "docker-compose up -d && next dev",
    "format": "prettier --ignore-unknown --write .",
    "format:check": "prettier --ignore-unknown --check .",
    "generate:importmap": "payload generate:importMap",
    "generate:types": "payload generate:types",
    "lint": "eslint . && tsc --noEmit",
    "migrate:create": "FORCE_MIGRATIONS=true payload migrate:create",
    "start": "pnpm payload migrate && node .next/standalone/server.js"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.74.0",
    "@payloadcms/next": "3.74.0",
    "@payloadcms/richtext-lexical": "3.74.0",
    "@payloadcms/storage-azure": "3.74.0",
    "@payloadcms/ui": "3.74.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tietokilta/ilmomasiina-client": "3.0.0-alpha.7",
    "@tietokilta/ilmomasiina-models": "3.0.0-alpha.7",
    "graphql": "^16.12.0",
    "lucide-react": "^0.541.0",
    "mdast": "^3.0.0",
    "next": "16.1.6",
    "next-intl": "^4.7.0",
    "payload": "3.74.0",
    "react": "19.2.4",
    "react-countdown": "^2.3.6",
    "react-dom": "19.2.4",
    "react-dvd-screensaver": "^0.1.1",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.5",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.19.27",
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "prettier": "^3.7.4",
    "prettier-plugin-packagejson": "^2.5.20",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.11.1+sha512.e519b9f7639869dc8d5c3c5dfef73b3f091094b0a006d7317353c72b124e80e1afd429732e28705ad6bfa1ee879c1fce46c128ccebd3192101f43dd67c667912",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ],
    "overrides": {
      "@types/react": "19.2.10",
      "@types/react-dom": "19.2.3"
    }
  }
}
