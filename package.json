{
  "name": "juvusivu",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build --experimental-build-mode compile",
    "codegen": "pnpm generate:importmap && pnpm generate:types",
    "db:seed": "payload run src/lib/api/standaloneSeed.ts",
    "dev": "docker-compose up -d && next dev --turbopack",
    "format": "prettier --ignore-unknown --write .",
    "format:check": "prettier --ignore-unknown --check .",
    "generate:importmap": "payload generate:importMap",
    "generate:types": "payload generate:types",
    "lint": "next lint",
    "migrate:create": "FORCE_MIGRATIONS=true payload migrate:create",
    "start": "pnpm payload migrate && node .next/standalone/server.js"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "^3.54.0",
    "@payloadcms/next": "^3.54.0",
    "@payloadcms/richtext-lexical": "^3.54.0",
    "@payloadcms/storage-azure": "^3.54.0",
    "graphql": "^16.11.0",
    "lucide-react": "^0.541.0",
    "next": "15.5.2",
    "next-international": "^1.3.1",
    "payload": "^3.54.0",
    "react": "19.1.1",
    "react-countdown": "^2.3.6",
    "react-dom": "19.1.1",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.19.11",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "eslint": "^9.34.0",
    "eslint-config-next": "15.5.2",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@10.11.1+sha512.e519b9f7639869dc8d5c3c5dfef73b3f091094b0a006d7317353c72b124e80e1afd429732e28705ad6bfa1ee879c1fce46c128ccebd3192101f43dd67c667912",
  "pnpm": {
    "overrides": {
      "@types/react": "19.1.12",
      "@types/react-dom": "19.1.9"
    }
  }
}
